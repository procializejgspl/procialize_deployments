<h1>Listing event_days</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Conference</th>
    <th>Sequence</th>
    <th>Event date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @event_days.each do |event_day| %>
  <tr>
    <td><%= event_day.name %></td>
    <td><%= event_day.conference_id %></td>
    <td><%= event_day.sequence %></td>
    <td><%= event_day.event_date %></td>
    <td><%= link_to 'Show', event_day %></td>
    <td><%= link_to 'Edit', edit_event_day_path(event_day) %></td>
    <td><%= link_to 'Destroy', event_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event day', new_event_day_path %>
